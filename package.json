{
  "name": "dealer365-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:api-crm": "nest build api-crm",
    "build:job-crm": "nest build job-crm",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "concurrently \"npm:dev:*\"",
    "dev:api-crm": "cross-env NODE_ENV=development nest start --watch api-crm",
    "dev:job-crm": "cross-env NODE_ENV=development nest start --watch job-crm",
    "start:debug": "concurrently \"npm:debug:*\"",
    "debug:api-crm": "cross-env NODE_ENV=development nest start --debug --watch api-crm",
    "debug:job-crm": "cross-env NODE_ENV=development nest start --debug --watch job-crm",
    "start:prod": "node dist/apps/dealer365-backend/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/dealer365-backend/test/jest-e2e.json",
    "docker:api-crm": "docker build -t dealer365-api-crm -f dockerfile.api-crm .",
    "docker:job-crm": "docker build -t dealer365-job-crm -f dockerfile.job-crm .",
    "depcruise:setup": "npx dependency-cruiser --init",
    "depcruise:api-crm": "depcruise --output-type dot apps\\api-crm\\src | dot -T svg > dependency-graph.svg"
  },
  "dependencies": {
    "@azure/service-bus": "^7.9.5",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "@sentry/node": "^8.34.0",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "concurrently": "^9.0.1",
    "helmet": "^8.0.0",
    "jwt-decode": "^4.0.0",
    "keycloak-connect": "^26.0.0",
    "mongoose": "^8.7.1",
    "nest-winston": "^1.9.7",
    "nestjs-pino": "^4.1.0",
    "pino": "^9.4.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^dealer365-backend/shared(|/.*)$": "<rootDir>/libs/shared/src/$1",
      "^dealer365-backend/nest(|/.*)$": "<rootDir>/libs/nest/src/$1",
      "^@dealer365-backend/nest-common(|/.*)$": "<rootDir>/libs/nest-common/src/$1",
      "^@dealer365-backend/package-crm(|/.*)$": "<rootDir>/libs/package-crm/src/$1",
      "^@dealer365-backend/package-product(|/.*)$": "<rootDir>/libs/package-product/src/$1",
      "^@dealer365-backend/message-broker(|/.*)$": "<rootDir>/libs/message-broker/src/$1",
      "^@dealer365-backend/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@dealer365-backend/custom-logger(|/.*)$": "<rootDir>/libs/custom-logger/src/$1"
    }
  }
}
